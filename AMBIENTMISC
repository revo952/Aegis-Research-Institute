local Sound1 = script.Sound1
local Sound2 = script.Sound2
local Sound3 = script.Sound3
local Sound4 = script.Sound4
local Sound5 = script.Sound5
local Sound6 = script.Sound6
local Sound7 = script.Sound7
local Sound8 = script.Sound8
local Sound9 = script.Sound9

local S = 0

while true do
	wait(math.random(10,35))
	S = math.random(1,13)
	if S == 1 then
		Sound1:Play()
	end
	if S == 2 then
		Sound2:Play()
	end
	if S == 3 then
		Sound3:Play()
	end
	if S == 4 then
		Sound4:Play()
	end
	if S == 5 then
		Sound5:Play()
	end
	if S == 6 then
		Sound6:Play()
	end
	if S == 7 then
		Sound7:Play()
	end
	if S == 8 then
		Sound8:Play()
	end
	if S == 9 then
		Sound9:Play()
	end
end
